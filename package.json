{
  "name": "apex-app",
  "author": {
    "name": "Purukitto",
    "url": "https://github.com/Purukitto"
  },
  "license": "GNU GPL-3.0",
  "homepage": "https://github.com/Purukitto/apex-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Purukitto/apex-app.git"
  },
  "bugs": {
    "url": "https://github.com/Purukitto/apex-app/issues"
  },
  "version": "1.7.3",
  "type": "module",
  "scripts": {
    "predev": "node scripts/inject-firebase-sw.js && node scripts/inject-google-services.js",
    "dev": "vite --host",
    "prebuild": "node scripts/inject-firebase-sw.js && node scripts/inject-google-services.js",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "cap:build": "npx cap sync",
    "dev:android": "npx cap run android --live-reload --host=192.168.1.9 --port=5173",
    "assets:android": "npx capacitor-assets generate --android --iconBackgroundColor #0A0A0A --splashBackgroundColor #0A0A0A",
    "log:android": "adb logcat -c; adb logcat | Select-String -Pattern \"Motion|lean|accel|Capacitor|Console\" -CaseSensitive:$false",
    "log:android:motion": "adb logcat -c; adb logcat chromium:V Capacitor:V Console:V *:E | Select-String -Pattern \"Motion|lean|accel|DeviceMotion|sensor\" -CaseSensitive:$false",
    "preview": "vite preview",
    "prepare": "husky",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "release:first-release": "standard-version --first-release"
  },
  "dependencies": {
    "@capacitor-community/file-opener": "^8.0.0",
    "@capacitor/android": "^8.0.2",
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@capacitor/file-transfer": "^2.0.3",
    "@capacitor/filesystem": "^8.1.0",
    "@capacitor/geolocation": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.2",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/motion": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/share": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@capgo/inappbrowser": "^8.1.12",
    "@danyalwe/capacitor-sensors": "^1.0.1",
    "@emotion/is-prop-valid": "^1.4.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "^2.93.3",
    "@tanstack/react-query": "^5.90.20",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^12.8.0",
    "framer-motion": "^12.30.0",
    "html2canvas": "^1.4.1",
    "loglevel": "^1.9.2",
    "lucide-react": "^0.563.0",
    "maplibre-gl": "^5.17.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1"
  }
}
